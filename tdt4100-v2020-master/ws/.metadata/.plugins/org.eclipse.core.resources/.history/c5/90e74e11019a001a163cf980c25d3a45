package EksamenKont2018;

public class DiceScore {
	
	private final Dice scoringDice;
	private final int score;
	
	public DiceScore(Dice scoringDice, int score) {
        this.scoringDice=scoringDice;
        this.score=score;
    }	
	
	
	
	
	public Dice getScoringDice() {
		return this.scoringDice;
	}
	
	public int getScore() {
		return this.score;
	}
	
	
}

package Eksamen2016;

import java.util.ArrayList;
import java.util.Collection;

public class Sibling implements Relation{

	@Override
	public Collection<Person> getRelativesOf(Relation relation) {
		if()
		Collection<Person> sibling = new ArrayList<>();
		if(person.getFather()!=null) {
			for(Person child:person.getFather().getChildren(Gender.FEMALE)) {
				sibling.add(child);
			}
		}
		if(person.getMother()!=null) {
			for(Person child:person.getMother().getChildren(Gender.FEMALE)) {
				if(!sisters.contains(child)) {
					sisters.add(child);
				}
			}
		}
		return sisters;
	}

}

package EksamenKont2015;

import org.junit.Test;

import junit.framework.TestCase;

public class ValueTest extends TestCase{
	Unit unit = new Unit("m");
	String double1 = "2.1m";
	String double2 = "12.1";
	String expectdFail1 = "fail";
	String s = null;
	
	@Test
	public void testValueOf_expetcingTrue() {
		Value temp = new Value(unit,2.1);
		assertTrue(temp.toString().equals(Value.valueOf(double1).toString()));
	}
	

	@Test
	public void testValueOf_expectingException() {
		try {
			Value.valueOf(s);
			fail("Should throw expection when entering av value null");
		}catch(IllegalArgumentException e) {
			
		}
	}
	
	
	
}

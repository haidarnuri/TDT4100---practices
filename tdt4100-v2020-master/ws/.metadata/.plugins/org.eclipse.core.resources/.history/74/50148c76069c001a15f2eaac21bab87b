package EksamenKont2018;

public abstract class AbstractScorer{

	private int scoreValue;
	public AbstractScorer(int score) {
		this.scoreValue=score;
	}
	
	
	public int getScoreValue() {
		return scoreValue;
	}
	
	
	public DiceScore getScore()Dice dice {
		
	}
	
	
	
	public abstract DiceScore internalScoreCounter();
	
	
	
	
}

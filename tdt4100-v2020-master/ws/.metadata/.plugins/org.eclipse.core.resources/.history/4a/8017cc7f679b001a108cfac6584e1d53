package Eksamen2016;

import java.util.ArrayList;
import java.util.Collection;

public class Sister extends Sibling implements Relation{
	
	public Sister() {
		super(Gender.FEMALE);
	}
	
	//Går via en av foreldrene til barnet og finner alle barnene. 
	@Override
	public Collection<Person> getRelativesOf(Person person) {
		super(person);
	}

	
}

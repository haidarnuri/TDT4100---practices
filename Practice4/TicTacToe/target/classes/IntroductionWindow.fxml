<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="247.0" prefWidth="81.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.IntroductionController">
    <children>
        <VBox layoutX="4.0" layoutY="4.0" prefHeight="400.0" prefWidth="200.0">
            <children>
                <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="50.0" prefWidth="208.0" text="Welcome to TicTacToe!" textFill="#6e3fcc" />
                <TextField fx:id="writeName" promptText="Write your name" />
                <Button fx:id="newGame" mnemonicParsing="false" onAction="#start" prefHeight="150.0" prefWidth="200.0" text="Start game" />
                <Button fx:id="exit" mnemonicParsing="false" onAction="#exitGame" prefHeight="150.0" prefWidth="200.0" text="Exit" />
            </children>
        </VBox>
    </children>
</AnchorPane>
